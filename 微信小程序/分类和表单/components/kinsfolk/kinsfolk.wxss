.flex-between {
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.item {
  box-sizing: border-box;
  width: 100%;
  background-color: #fff;
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 10rpx 0;
}
.itemValue {
  flex: 1;
  text-align: right;
}
.picker {
  width: 100%;
  height: 100%;
  color: #808080;
}
.pickerValue {
  color: #000;
}
.kinsfolk {
  background-color: #eee;
  margin: 20rpx 0;
  padding: 0 20rpx 10rpx;
}
.kinsfolkTitle {
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 15rpx 0;
}
.kinsfolkOs {
  width: 40rpx;
  height: 40rpx;
  background-color: #fff;
  border-radius: 50%;
  text-align: center;
  font-size: 36rpx;
  line-height: 39rpx;
}
.kinsfolkContent .item {
  padding: 10rpx 15rpx;
}